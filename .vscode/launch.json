        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
        ,
        {
            "version": "0.2.0",
            "configurations": [
              {
                "name": "Attach to Python Functions",
                "type": "python",
                "request": "attach",
                "port": 9091,
                "preLaunchTask": "func: host start"
              }
            ]
          }
          
    ]
}